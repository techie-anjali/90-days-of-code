#include <iostream>
#include <vector>
using namespace std;

void f(int i, vector<int>& arr, int n) {
    if (i >= n / 2) return;
    swap(arr[i], arr[n - i - 1]);
    f(i + 1, arr, n);
}

int main() {
    int n;
    cin >> n;

    vector<int> arr;
    int temp;
    for (int i = 0; i < n; i++) {
        cin >> temp;
        arr.push_back(temp);
    }

    f(0, arr, n);

    for (int i = 0; i < n; i++) {
        cout << arr[i] << " ";
    }

    cout << endl; // âœ… fixed
    return 0;
}



//progam to check if a given string is palindrome ot not //
class Solution {
public:
    bool check(int i, const string &s) {
        if (i >= s.size() / 2) return true;
        if (s[i] != s[s.size() - i - 1]) return false;
        return check(i + 1, s);
    }

    bool isPalindrome(string s) {
        string filtered = "";
        for (char c : s) {
            if (isalnum(c))
                filtered += tolower(c);
        }
        return check(0, filtered); 
    }
};
