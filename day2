#include <iostream>
#include <vector>
using namespace std;

void f(int i, vector<int>& arr, int n) {
    if (i >= n / 2) return;
    swap(arr[i], arr[n - i - 1]);
    f(i + 1, arr, n);
}

int main() {
    int n;
    cin >> n;

    vector<int> arr;
    int temp;
    for (int i = 0; i < n; i++) {
        cin >> temp;
        arr.push_back(temp);
    }

    f(0, arr, n);

    for (int i = 0; i < n; i++) {
        cout << arr[i] << " ";
    }

    cout << endl; // âœ… fixed
    return 0;
}
